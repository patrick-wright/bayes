<h1>Listing probability_results</h1>

<table>
  <thead>
    <tr>
      <th>Received training</th>
      <th>No training</th>
      <th>Attended performance increase</th>
      <th>Attended no performance increase</th>
      <th>Unattended performance increase</th>
      <th>Nattended no performance increase</th>
      <th>Prob received training to attended performance increase</th>
      <th>Prob received training to attended no performance increase</th>
      <th>Prob no training to unattended performance increase</th>
      <th>Prob no training to unattended no performance increase</th>
      <th>Probability</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @probability_results.each do |probability_result| %>
      <tr>
        <td><%= probability_result.received_training %></td>
        <td><%= probability_result.no_training %></td>
        <td><%= probability_result.attended_performance_increase %></td>
        <td><%= probability_result.attended_no_performance_increase %></td>
        <td><%= probability_result.unattended_performance_increase %></td>
        <td><%= probability_result.nattended_no_performance_increase %></td>
        <td><%= probability_result.prob_received_training_to_attended_performance_increase %></td>
        <td><%= probability_result.prob_received_training_to_attended_no_performance_increase %></td>
        <td><%= probability_result.prob_no_training_to_unattended_performance_increase %></td>
        <td><%= probability_result.prob_no_training_to_unattended_no_performance_increase %></td>
        <td><%= probability_result.probability %></td>
        <td><%= link_to 'Show', probability_result %></td>
        <td><%= link_to 'Edit', edit_probability_result_path(probability_result) %></td>
        <td><%= link_to 'Destroy', probability_result, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Probability result', new_probability_result_path %>
